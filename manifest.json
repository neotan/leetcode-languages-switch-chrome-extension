{
  "name": "Leetcode languages switcher",
  "description": "Switching between English and Chinese sites.",
  "version": "0.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "*://*.leetcode.com/*",
        "*://*.leetcode.cn/*"
      ],
      "run_at": "document_idle"
    }
  ]
}